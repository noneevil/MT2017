<?xml version="1.0" encoding="utf-8" ?>
<CodeSnippet Format="1.0.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>ExecuteObject.UPDATE</Title>
    <Author>noneevil</Author>
    <Shortcut>update</Shortcut>
    <Description>ExecuteObject 更新表数据</Description>
    <SnippetTypes>
      <SnippetType>SurroundsWith</SnippetType>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>obj</ID>
        <Default>obj</Default>
      </Literal>
    </Declarations>
    <Code Language="CSharp">
      <![CDATA[
        ExecuteObject $obj$ = new ExecuteObject();
        $obj$.cmdtype = CmdType.UPDATE;
        $obj$.tableName = "表名";
        $obj$.terms.Add("ID", 0);//主键
        $obj$.cells.Add("更新字段", "数据值");
        if (db.ExecuteCommand($obj$))
        {
          //数据更新成功！
        }
      ]]>
    </Code>
  </Snippet>
</CodeSnippet>